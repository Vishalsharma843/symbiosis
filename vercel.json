{ 
  "version":2,
    "builds": [
      {
        "scripts": {
          "build": " build --output public"
        }
      },
      {
        "src": "blogverse/wsgi.py",
        "use": "@vercel/python",
        "config":{ "maxLambdaSizen": "15mb", "runtime":"python3.11.5" }
      },
      {
        "src":"build_files.sh",
        "use":"@vercel/static-build",
        "config":{
            "disDir":"staticfiles_build"
        }
      }
    ],
    "routes": [
      {
        "src": "/static/(.*)",
        "dest": "/static/$1"
      },
      {
        "src":"/(.*)",
        "dest":"blogverse/wsgi.py"
      }
    ]
}
